; Function Attrs: nounwind uwtable
define i32 @factorial(i32 %x) #0 {
  %1 = alloca i32, align 4
  store i32 %x, i32* %1, align 4
  %2 = load i32, i32* %1, align 4
  %3 = load i32, i32* %1, align 4
  %4 = sub nsw i32 %3, 1
  %5 = call i32 @factorial(i32 %4)
  %6 = mul nsw i32 %2, %5
  ret i32 %6
}

